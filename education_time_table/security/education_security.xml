<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.rule" id="faculty_timetable_schedule_rule">
        <field name="name">Faculty Timetable Schedule rule</field>
        <field name="model_id" ref="education_time_table.model_education_timetable_schedule" />
        <field name="groups"
            eval="[(4, ref('education_core.group_education_faculty'))]" />
        <field name="domain_force"> [('faculty_id.employee_id.user_id','=',user.id)]</field>
    </record>

    <record model="ir.rule" id="hod_timetable_schedule_rule">
        <field name="name">Hod Timetable Schedule rule</field>
        <field name="model_id" ref="education_time_table.model_education_timetable_schedule" />
        <field name="groups"
            eval="[(4, ref('education_core.group_education_hod'))]" />
        <field name="domain_force"> []</field>
    </record>

</odoo>
